        <rdf:type rdf:resource="http://www.metaclip.org/datasource/datasource.owl#ModellingCenter"/>
        <rdfs:comment xml:lang="en">Canadian Centre for Climate Modelling and Analysis</rdfs:comment>
        <rdfs:label xml:lang="en">CCCma</rdfs:label>
        <rdfs:seeAlso xml:lang="en">https://www.canada.ca/en/environment-climate-change/services/climate-change/centre-modelling-analysis/models.html</rdfs:seeAlso>
    </owl:NamedIndividual>
    


