    <!-- http://metaclip.predictia.es/datasource/datasource.owl#ENSO1plus2 -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/datasource/datasource.owl#ENSO1plus2">
        <rdf:type rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#ENSOregion"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Predictia</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-03-05T12:22:45Z</dc:date>
        <dc:description xml:lang="en">The Ni√±o 1+2 region is the smallest and eastern-most of the Ni√±o SST regions, and corresponds with the region of coastal South America where El Ni√±o was first recognized by the local populations.  This index tends to have the largest variance of the Ni√±o SST indices</dc:description>
        <geosparql:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POLYGON((-90 10, -80 -10, -80 0, -90 0, -90 10))</geosparql:asWKT>
        <rdfs:seeAlso xml:lang="en">https://climatedataguide.ucar.edu/climate-data/nino-sst-indices-nino-12-3-34-4-oni-and-tni</rdfs:seeAlso>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/datasource/datasource.owl#ENSO3 -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/datasource/datasource.owl#ENSO3">
        <rdf:type rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#ENSOregion"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Predictia</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-03-05T12:22:28Z</dc:date>
        <dc:description xml:lang="en">This region was once the primary focus for monitoring and predicting El Ni√±o, but researchers later learned that the key region for coupled ocean-atmosphere interactions for ENSO lies further west (Trenberth, 1997).  Hence, the Ni√±o 3.4 and ONI became favored for defining El Ni√±o and La Ni√±a events</dc:description>
        <geosparql:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POLYGON((-150 -5, -90 -5, -90 5, -150 5, -150 -5))</geosparql:asWKT>
        <rdfs:seeAlso xml:lang="en">https://climatedataguide.ucar.edu/climate-data/nino-sst-indices-nino-12-3-34-4-oni-and-tni</rdfs:seeAlso>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/datasource/datasource.owl#ENSO3dot4 -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/datasource/datasource.owl#ENSO3dot4">
        <rdf:type rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#ENSOregion"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Predictia</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-03-05T12:14:44Z</dc:date>
        <dc:description xml:lang="en">The  Ni√±o 3.4 anomalies may be thought of as representing the average equatorial SSTs across the Pacific from about the dateline to the South American coast.  The Ni√±o 3.4 index typically uses a 5-month running mean, and El Ni√±o or La  Ni√±a events are defined when the  Ni√±o 3.4 SSTs exceed +/- 0.4C for a period of six months or more</dc:description>
        <geosparql:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POLYGON((-170 -5, -120 -5, -120 5, -170 5, -170 -5))</geosparql:asWKT>
        <rdfs:seeAlso xml:lang="en">https://climatedataguide.ucar.edu/climate-data/nino-sst-indices-nino-12-3-34-4-oni-and-tni</rdfs:seeAlso>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/datasource/datasource.owl#ENSO4 -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/datasource/datasource.owl#ENSO4">
        <rdf:type rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#ENSOregion"/>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Predictia</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-03-05T12:22:36Z</dc:date>
        <dc:description xml:lang="en">The  Ni√±o 4 index captures SST anomalies in the central equatorial Pacific.  This region tends to have less variance than the other Ni√±o regions</dc:description>
        <geosparql:asWKT rdf:datatype="http://www.opengis.net/ont/geosparql#wktLiteral">POLYGON((160 -5, -150 -5, -150 5, 160 5, 160 -5))</geosparql:asWKT>
        <rdfs:seeAlso xml:lang="en">https://climatedataguide.ucar.edu/climate-data/nino-sst-indices-nino-12-3-34-4-oni-and-tni</rdfs:seeAlso>
    </owl:NamedIndividual>
    


