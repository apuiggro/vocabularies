    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#hasProbCharacter -->

    <owl:DatatypeProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#hasProbCharacter">
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>deterministic</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>stochastic</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#isMultiSite -->

    <owl:DatatypeProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#isMultiSite">
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#isMultiVariable -->

    <owl:DatatypeProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#isMultiVariable">
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <dc:description xml:lang="en">Bias correction (or &apos;adjustment&apos;) techniques adjust the raw model output distribution towards the observed one to ensure distributional resemblance to the local climatology. The simplest techniques adjust the mean/variance of the distribution (e.g. :LinearScaling), while more complex (parametric or empirical) quantile mapping techniques adjust the whole distribution (e.g. :EQM).</dc:description>
        <rdfs:comment xml:lang="en">The key difference of this class with ESD-MOS is that bias adjustment has a limited capability for downscaling (i.e., unsuitable to bring local scale variability into the adjusted results), as it does not establish a temporal connection between model variables and observations). The more specific subclasses should be asserted when relevant.</rdfs:comment>
        <rdfs:label>BiasCorrection</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#CCA -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#CCA">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ESD-MOS"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#Calibration -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#Calibration">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#Step"/>
        <dc:description xml:lang="en">Adjustment techniques are a broad family of empirical methods aiming at the statistical transformation of a climate model output using as reference one or several variables from a second dataset.</dc:description>
        <rdfs:label>Calibration</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#ESD -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#ESD">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <dc:description xml:lang="en">A family of Model Output Adjustment techniques based on statistical models linking suitable informative model outputs (predictors, typically middle tropospheric variables) to the local observed predictand of interest over a particular domain, working at a daily to seasonal scale. These models are first trained using model and observed data for a representrative historical reanalysis/hindcast period, and later applied to new (e.g. predicted) GCM/RCM data to obtain the downscaled local predictions.</dc:description>
        <rdfs:label>ESD</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#ESD-MOS -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#ESD-MOS">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ESD"/>
        <owl:disjointWith rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ESD-PP"/>
        <dc:description xml:lang="en">Under the Model Output Statistics (MOS) approach, model outputs from the GCM/RCM are directly used for model calibration, thus correcting systematic biases against observations.</dc:description>
        <rdfs:label xml:lang="en">Empirical Statistical Downscaling under the MOS approach</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#ESD-PP -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#ESD-PP">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ESD"/>
        <dc:description xml:lang="en">Under the Perfect-prog (PP) approach, quasi-observations from a reanalysis are used in the model calibration phase based on their temporal (e.g. daily or seasonal) correspondence with observations in the historical trainig period.</dc:description>
        <rdfs:label xml:lang="en">Empirical Statistical Downscaling under the Perfect Prog approach</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#EnsembleRecalibration -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#EnsembleRecalibration">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <dc:description xml:lang="en">Broadly speaking, the main goal of Ensemble Recalibration techniques is producing reliable probability forecasts by directly modifying the ensemble predictions (ensemble re-calibration) or derived products such as tercile probabilities (probability re-calibration).</dc:description>
        <rdfs:label>EnsembleRecalibration</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#LinearScaling -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#LinearScaling">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#NonParametricBiasCorrection"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#MOS-Analogs -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#MOS-Analogs">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ESD-MOS"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#NPQuantileMapping -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#NPQuantileMapping">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#NonParametricBiasCorrection"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#NonParametricBiasCorrection -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#NonParametricBiasCorrection">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#PQuantileMapping -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#PQuantileMapping">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ParametricBiasCorrection"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#ParametricBiasCorrection -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#ParametricBiasCorrection">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#ProbabilityScaling -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#ProbabilityScaling">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#EnsembleRecalibration"/>
        <owl:disjointWith rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#VarianceInflation"/>
        <rdfs:label>ProbabilityScaling</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#TransferFunctions -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#TransferFunctions">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ESD-PP"/>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#VarianceInflation -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#VarianceInflation">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#EnsembleRecalibration"/>
        <rdfs:label>VarianceInflation</rdfs:label>
    </owl:Class>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#WeatherTyping -->

    <owl:Class rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#WeatherTyping">
        <rdfs:subClassOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ESD-PP"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#BC -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#BC">
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ParametricBiasCorrection"/>
        <hasProbCharacter>deterministic</hasProbCharacter>
        <isMultiSite rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiSite>
        <isMultiVariable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiVariable>
        <dc:description xml:lang="en">Parametric bias correction method using the optimum among five theoretical distributions (Gamma, Weibull, classical Gumbel, reversed Gumbel and log-logistic, all of them with four parameters) for each station on a monthly basis (Monjo et al. 2014, &lt;https://doi.org/10.1007/s00704-013-1008-8&gt;).</dc:description>
        <rdfs:comment xml:lang="en">The method is described for precipitation only</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://doi.org/10.1002/joc.5462</rdfs:isDefinedBy>
        <rdfs:label>BC</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#CDFt -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#CDFt">
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#ParametricBiasCorrection"/>
        <hasProbCharacter>deterministic</hasProbCharacter>
        <isMultiSite rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiSite>
        <isMultiVariable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiVariable>
        <dc:description xml:lang="en">The CDFt approach links the local-scale CDF of the variable of interest to the associated large-scale CDF through a &apos;quantile–quantile&apos; approach performed between the future large- and local-scale CDFs (and not between present CDFs as in the classical :QuantileMapping methods. To do so, the future local-scale CDF is first estimated based on the assumption of a mathematical transformation to link the evolution of the large-scale CDF to the evolution of the local-scale one. Hence, CDFt is a variant of quantile–quantile but CDFt accounts for the CDF changes from the calibration to the projection (or future) time periods (Vrac et al. 2012, &lt;https://doi.org/10.5194/nhess-12-2769-2012&gt;).</dc:description>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://doi.org/10.1002/joc.5462</rdfs:isDefinedBy>
        <rdfs:label>CDFt</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#EQM -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#EQM">
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#NPQuantileMapping"/>
        <hasProbCharacter>deterministic</hasProbCharacter>
        <isMultiSite rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiSite>
        <isMultiVariable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiVariable>
        <dc:description xml:lang="en">Implementation of empirical quantile mapping (:EQM) adjusting 99 percentiles and linearly interpolating inside this range every two consecutive percentiles; outside this range a constant extrapolation (using the correction obtained for the 1st or 99th percentile) is applied (Déqué et al. 2007 &lt;https://doi.org/10.1016/j.gloplacha.2006.11.030&gt;). In the case of the precipitation, when the predicted dry frequency is larger than the observed one the frequency adaptation proposed by &lt;Themessl et al. 2010, https://doi.org/10.1007/s10584-011-0224-4&gt;.</dc:description>
        <rdfs:isDefinedBy xml:lang="en">https://doi.org/10.1002/joc.5462</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">EQM</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#EQM-WIC658 -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#EQM-WIC658">
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#NPQuantileMapping"/>
        <hasProbCharacter>deterministic</hasProbCharacter>
        <isMultiSite rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiSite>
        <isMultiVariable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiVariable>
        <dc:description xml:lang="en">Implementation of the empirical quantile mapping method (Déqué, 2007 &lt;https://doi.org/10.1016/j.gloplacha.2006.11.030&gt;) sorting the values into bins with adjustable width (e.g.: 0.1) and applying a linear interpolation between two percentiles (bins); out of range values are adjusted using constant extrapolation (using the correction obtained for the minimum or maximum). In order to cope with the seasonal cycle, a 31-day moving window centred on every calendar day is used to calibrate the method &lt;see Wilcke et al. 2013, https://doi.org/10.1007/s10584-013-0845-x&gt;</dc:description>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://doi.org/10.1002/joc.5462</rdfs:isDefinedBy>
        <rdfs:label>EQM-WIC658</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#EQMs -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#EQMs">
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#BiasCorrection"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <rdf:type rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#NPQuantileMapping"/>
        <hasProbCharacter>deterministic</hasProbCharacter>
        <isMultiSite rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiSite>
        <isMultiVariable rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</isMultiVariable>
        <dc:description xml:lang="en">This method is similar to :EQM. However, In order to explicitly model the seasonal cycle, the variant EQMs considers a 31-day moving window centred on every calendar day to calibrate the method</dc:description>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://doi.org/10.1002/joc.5462</rdfs:isDefinedBy>
        <rdfs:label>EQMs</rdfs:label>
        <prov:specializationOf rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#EQM"/>
    </owl:NamedIndividual>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#ISI-MIP -->

    <owl:NamedIndividual rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#ISI-MIP">
