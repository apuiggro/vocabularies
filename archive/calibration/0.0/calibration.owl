<?xml version="1.0"?>
<rdf:RDF xmlns="http://metaclip.predictia.es/calibration/calibration.owl#"
     xml:base="http://metaclip.predictia.es/calibration/calibration.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cal="http://metaclip.predictia.es/calibration/calibration.owl#"
     xmlns:ds="http://metaclip.predictia.es/datasource/datasource.owl#"
     xmlns:geosparql="http://www.opengis.net/ont/geosparql#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://metaclip.predictia.es/calibration/calibration.owl">
        <owl:versionIRI rdf:resource="http://metaclip.predictia.es/calibration/0.0/calibration.owl"/>
        <owl:imports rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl"/>
        <dc:description xml:lang="en">The calibration ontology is aimed at the creation of a conceptual framework to describe the different statistical calibration techniques and procedures used in climate data analysis. In particular, the calibration ontology deals with bias correction, perfect-prog, ensemble recalibration and similar statistical post-processing techniques that rely on an observed reference and a calibration dataset to undertake an adjustment of climate model outputs. The vocabulary is constructed following the naming conventions and techniques followed in several international initiatives, such as the Action COST VALUE and EURO-CORDEX.</dc:description>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.0</owl:versionInfo>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://metaclip.predictia.es/</rdfs:seeAlso>
        <dc:creator>Predictia</dc:creator>
        <dc:title xml:lang="en">The Calibration Vocabulary of METACLIP</dc:title>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#hasProbCharacter -->

    <owl:AnnotationProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#hasProbCharacter"/>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#isMultiSite -->

    <owl:AnnotationProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#isMultiSite">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
    </owl:AnnotationProperty>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#isMultiVariable -->

    <owl:AnnotationProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#isMultiVariable">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#hadCalibration -->

    <owl:ObjectProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#hadCalibration">
        <rdfs:subPropertyOf rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#hadTransformation"/>
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#Step"/>
        <rdfs:range rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#Calibration"/>
        <dc:description xml:lang="en">This object property links a ds:Dtep with a :Calibration step</dc:description>
        <rdfs:label>hadCalibration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#withCalibrationMethod -->

    <owl:ObjectProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#withCalibrationMethod">
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#Calibration"/>
        <rdfs:range rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#CalibrationMethod"/>
        <dc:description xml:lang="en">Object property linking a :Calibration step with the description of the :CalibrationMethod applied</dc:description>
        <rdfs:label>withCalibrationMethod</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#withReferenceData -->

    <owl:ObjectProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#withReferenceData">
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#Calibration"/>
        <rdfs:range rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#Step"/>
        <dc:description xml:lang="en">This object property links a :Calibration step with the reference data (generally observations) used as predictand in the calibration stage</dc:description>
        <rdfs:label>withReferenceData</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://metaclip.predictia.es/calibration/calibration.owl#withTrainingData -->

    <owl:ObjectProperty rdf:about="http://metaclip.predictia.es/calibration/calibration.owl#withTrainingData">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://metaclip.predictia.es/calibration/calibration.owl#Calibration"/>
        <rdfs:range rdf:resource="http://metaclip.predictia.es/datasource/datasource.owl#Step"/>
        <dc:description xml:lang="en">This object property links a :Calibration step with the training data (predictor set) used in the calibration stage</dc:description>
        <rdfs:label>withTrainingData</rdfs:label>
    </owl:ObjectProperty>
    


